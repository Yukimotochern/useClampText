{
  "name": "useclamptext",
  "version": "1.0.0",
  "description": "This package provide a custome hook that can limit the text in the container in specified line number.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run format && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yukimotochern/useClampText.git"
  },
  "keywords": [
    "React",
    "Hook",
    "Clamp",
    "Line Number"
  ],
  "author": "Yukimoto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yukimotochern/useClampText/issues"
  },
  "homepage": "https://github.com/Yukimotochern/useClampText#readme",
  "devDependencies": {
    "@types/react": "^18.0.27",
    "prettier": "^2.8.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "rooks": "^7.4.3"
  }
}
